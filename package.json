{
  "name": "contact-manager-cli",
  "version": "1.0.0",
  "description": "Manage your contacts from Command Line",
  "main": "index.js",
  "preferGlobal": true,
  "bin": "./contact.js",
  "scripts": {
    "serve": "firebase serve --only functions",
    "shell": "firebase experimental:functions:shell",
    "start": "npm run shell",
    "logs": "firebase functions:log",
    "deploy": "yarn run publish && firebase deploy --only functions",
    "prepublish": "rimraf firefunctions/functions",
    "publish": "babel firefunctions/functionsES6 --out-dir firefunctions/functions --presets=es2015 --copy-files --ignore 'node_modules'",
    "postpublish": "cd firefunctions/functions && yarn",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wyvarn/contact-manager-cli.git"
  },
  "keywords": [
    "Contact",
    "Contact",
    "CLI",
    "firebase",
    "google",
    "cloud",
    "functions"
  ],
  "author": "Brian Lusina",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wyvarn/contact-manager-cli/issues"
  },
  "homepage": "https://github.com/Wyvarn/contact-manager-cli#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "chalk": "^2.3.0",
    "commander": "^2.11.0",
    "core-js": "^2.5.1",
    "inquirer": "^4.0.0",
    "ora": "^1.3.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2"
  }
}
